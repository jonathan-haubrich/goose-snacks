generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model PriceModifiers {
  id               Int    @id @default(autoincrement())
  profitPercentage Float?
  roundingModifier Float?
}

model Products {
  id        Int     @id @default(autoincrement())
  votes     Int?
  name      String?
  imageUrl  String?
  fullPrice String?
  price     String?
}

model Votes {
  id          Int       @id @default(autoincrement())
  productId   Int?
  ipAddress   String?
  submittedAt DateTime? @default(now())
}
